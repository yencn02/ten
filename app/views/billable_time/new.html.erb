<div class="grid_16">
<h1>New billable_time</h1>

<%= form_for(:billable_time, :url => url_for(:action => "create")) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :task_id %><br />
    <%= f.text_field :task_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :billed_hour %><br />
    <%= f.text_field :billed_hour %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', "/billable_time" %>
</div>